{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "require-jsdoc" : 0,
        "no-unused-vars" : 1,
        "new-cap": 0,
        "max-len" : [1,{
            "ignoreStrings": true
        }]
    }
}